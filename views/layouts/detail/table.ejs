<div class="box">
    <div class="box-body">
        <div class="system-log-view">

            <p><a class="btn btn-primary" href="<%= template.back_link %>">Back</a></p>

            <table class="table table-striped table-bordered detail-view">
                <tbody>

                <% for(var cid in cols){ %>
                <tr>
                    <th><%= cols[cid].title %></th>
                    <td><%= model[cols[cid].id] %></td>
                </tr>
                <% } %>

                </tbody>
            </table>
        </div>
    </div>
</div>